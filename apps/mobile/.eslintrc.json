{
  "extends": [
    "plugin:@nrwl/nx/react",
    "../../.eslintrc.json"
  ],
  "env": {
    "es6": true,
    "jest": true
  },
  "ignorePatterns": [
    "!**/*",
    "public",
    ".cache",
    "node_modules"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "@typescript-eslint/ban-ts-comment": "off"
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}
